{{outlet}}
<div class="container">
  <div class="row placeholder"></div>
  <div class="panel panel-default">
    <div class="panel-body box">


  <strong class='welcome'>Welcome to Ember.js</strong>
  {{#each model as |message|}}

  <div class="row">
    <div class="col-sm-1 img-circle">
      <img src="{{message.pic}}" class='img-circle'><br>
    </div>
    <div class="col-md-11">
      <div class="row equalizer"></div>
      <div class="row message-text">
        <strong class="">{{message.message}}</strong>
      </div>
      <div class="row info">
        <strong class='info'>{{format-username message.username}}</strong>
      </div>
      <div class="row info">
        <strong class='info'>{{message.useragent}}</strong>
      </div>
      <div class="row">
        <p class='grey verdana'>{{message.date}}</p>
      </div>
    </div>
  </div>

  {{/each}}
  </div>
  </div>
</div>
